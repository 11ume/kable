{
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "ordered-imports": false,
        "trailing-comma": false,
        "quotemark": [
            true,
            "single"
        ],
        "semicolon": [
            true,
            "never"
        ],
        "interface-name": false,
        "curly": false,
        "prefer-for-of": false,
        "max-line-length": false,
        "object-literal-sort-keys": false,
        "align": false,
        "interface-over-type-literal": false,
        "member-access": false,
        "unified-signatures": false,
        "eofline": false,
        "no-unused-expression": false,
        "variable-name": false,
        "max-classes-per-file": false,
        "no-console": false,
        "no-bitwise": false,
        "no-shadowed-variable": [
            true,
            {
                "class": true,
                "enum": true,
                "function": true,
                "interface": false,
                "namespace": true,
                "typeAlias": false,
                "typeParameter": false,
                "temporalDeadZone": false
            }
        ]
    }
}